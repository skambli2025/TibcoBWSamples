<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns="http://www.tibco.com/pe/DeployedVarsType">
    <pd:name>processInSubsets.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startX>51</pd:startX>
    <pd:startY>129</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>529</pd:endX>
    <pd:endY>129</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1751602109236</pd:targetNamespace>
    <pd:group name="ProcessInSubsets">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>127</pd:x>
        <pd:y>13</pd:y>
        <pd:width>342</pd:width>
        <pd:height>213</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>repeat</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:indexSlot>i</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:repeatCondition>$querySrcOrderTab/resultSet/lastSubset = "true"</pd:repeatCondition>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>querySrcOrderTab</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>ProcessSubset</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>querySrcOrderTab</pd:from>
            <pd:to>ProcessSubset</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="querySrcOrderTab">
            <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
            <pd:x>214</pd:x>
            <pd:y>128</pd:y>
            <config>
                <timeout>10</timeout>
                <commit>false</commit>
                <maxRows>100</maxRows>
                <emptyStrAsNil>false</emptyStrAsNil>
                <jdbcSharedConfig>/TIBCO_Oracle.sharedjdbc</jdbcSharedConfig>
                <statement>SELECT * FROM SRC_ORDER_TABLE</statement>
                <QueryOutputCachedSchemaColumns>ORDER_ID</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>2</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
                <QueryOutputCachedSchemaColumns>ORDER_DESCRIPTION</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
                <QueryOutputCachedSchemaColumns>ORDER_PRICE</QueryOutputCachedSchemaColumns>
                <QueryOutputCachedSchemaDataTypes>2</QueryOutputCachedSchemaDataTypes>
                <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
                <processInSubsets>true</processInSubsets>
            </config>
            <pd:inputBindings>
                <jdbcQueryActivityInput>
                    <subsetSize>
                        <xsl:value-of select="$_globalVariables/ns:GlobalVariables/SUBSET_SIZE"/>
                    </subsetSize>
                </jdbcQueryActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:group name="ProcessSubset">
            <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
            <pd:resourceType>ae.process.group</pd:resourceType>
            <pd:x>283</pd:x>
            <pd:y>48</pd:y>
            <pd:width>142</pd:width>
            <pd:height>142</pd:height>
            <pd:collapsedWidth>57</pd:collapsedWidth>
            <pd:collapsedHeight>70</pd:collapsedHeight>
            <pd:joinToggle>inherit</pd:joinToggle>
            <config>
                <pd:groupType>inputLoop</pd:groupType>
                <pd:serializable>false</pd:serializable>
                <pd:over>$querySrcOrderTab/resultSet/Record</pd:over>
                <pd:indexSlot>j</pd:indexSlot>
                <pd:activityOutputName/>
                <pd:accumulateOutput>false</pd:accumulateOutput>
            </config>
            <pd:inputBindings/>
            <pd:expanded>true</pd:expanded>
            <pd:transition>
                <pd:from>start</pd:from>
                <pd:to>echoOneRecord</pd:to>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:transition>
                <pd:from>echoOneRecord</pd:from>
                <pd:to>end</pd:to>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:activity name="echoOneRecord">
                <pd:type>com.tibco.plugin.file.FileWriteActivity</pd:type>
                <pd:resourceType>ae.activities.FileWriteActivity</pd:resourceType>
                <pd:x>353</pd:x>
                <pd:y>128</pd:y>
                <config>
                    <encoding>text</encoding>
                    <compressFile>None</compressFile>
                    <append>true</append>
                </config>
                <pd:inputBindings>
                    <ns0:WriteActivityInputTextClass xmlns:ns0="http://www.tibco.com/namespaces/tnt/plugins/file">
                        <fileName>
                            <xsl:value-of select="$_globalVariables/ns:GlobalVariables/PROCESS_IN_SUBSETS_OUTPUT_FILE"/>
                        </fileName>
                        <textContent>
                            <xsl:value-of select="concat(&quot;Record: [&quot;, $querySrcOrderTab/resultSet/Record[$j]/ORDER_ID, &quot;, '&quot;, $querySrcOrderTab/resultSet/Record[$j]/ORDER_DESCRIPTION, &quot;', &quot;, $querySrcOrderTab/resultSet/Record[$j]/ORDER_PRICE, &quot;]&#xA;&quot;)"/>
                        </textContent>
                    </ns0:WriteActivityInputTextClass>
                </pd:inputBindings>
            </pd:activity>
        </pd:group>
    </pd:group>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>ProcessInSubsets</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>ProcessInSubsets</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>